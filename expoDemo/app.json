{
  "name": "react-native-ui-lib",
  "displayName": "rnuilib Demo",
  "expo": {
    "name": "rnuilib",
    "slug": "rnuilib",
    "privacy": "public",
    "description": "Demo project show case for react-native-ui-lib components",
    "githubUrl": "https://github.com/wix/react-native-ui-lib",
    "version": "2.0.0",
